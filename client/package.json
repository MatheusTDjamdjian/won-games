{
  "name": "boilerplate-apps-router",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rimraf .next",
    "dev": "next dev",
    "build": "next build -d",
    "test": "jest --passWithNoTests --maxWorkers=50%",
    "test:watch": "jest --watch --maxWorkers=25%",
    "test:ci": "jest --runInBand",
    "schema:download": "rover graph introspect http://localhost:1337/graphql > schema-temp.gql",
    "schema:convert": "node convertToNonNull.js",
    "codegen": "graphql-codegen --config codegen.yml",
    "types:generate": "npm run schema:download && npm run schema:convert && npm run codegen",
    "generate": "npx plop --plopfile generators/plopfile.js",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@apollo/client": "^3.13.9",
    "@styled-icons/fa-brands": "^10.47.0",
    "dotenv": "^16.5.0",
    "graphql": "^16.11.0",
    "jest-environment-jsdom": "^29.7.0",
    "next": "^15.2.4",
    "polished": "^4.3.1",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-slick": "^0.30.3",
    "slick-carousel": "^1.8.1",
    "styled-components": "^6.1.18",
    "styled-icons": "^10.47.1",
    "styled-media-query": "^2.1.2",
    "swr": "^2.3.3",
    "typescript": "5.2"
  },
  "devDependencies": {
    "@apollo/rover": "^0.35.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.24.0",
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@graphql-codegen/typescript-react-apollo": "^4.3.3",
    "@jridgewell/source-map": "^0.3.10",
    "@storybook/nextjs": "^10.0.7",
    "@storybook/react-vite": "^10.0.7",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.1",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/react-slick": "^0.23.13",
    "@types/styled-components": "^5.1.34",
    "babel-plugin-styled-components": "^2.1.4",
    "cross-env": "^7.0.3",
    "eslint": "^9.24.0",
    "eslint-config-next": "^15.1.6",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-storybook": "^10.0.7",
    "globals": "^15.15.0",
    "graceful-fs": "^4.2.11",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-styled-components": "^7.2.0",
    "plop": "^4.0.1",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "storybook": "^10.0.7",
    "typescript-eslint": "^8.29.0"
  },
  "type": "module",
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  },
  "overrides": {
    "storybook": "$storybook"
  }
}
